#Create factors
d$Group = factor(d$Group,levels = c('NAL', 'PLA', 'ALC'))
d$Group2 = factor(d$Group2,levels = c('CON', 'ALC'))
d$Sex = factor(d$Sex,levels  = c('M', 'F'))
d$UType = factor(d$UType, levels = c('P', 'A')) #Uncertainty type: Probabilistic, Ambiguous
d$Variance = factor(d$Variance, levels  = c('L', 'H')) #Variance: Low, high
# set contrasts
contrasts(d$Group) = varContrasts(d$Group, Type='POC',
